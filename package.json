{
    "name": "squadcalc",
    "version": "10.2.0",
    "description": "A Minimalist Mortar Calculator for Squad",
    "author": "Maxime 'sharkman' Boussard ",
    "license": "MIT",
    "keywords": [
        "squad",
        "squadcalc",
        "mortar",
        "calculator",
        "minimalist"
    ],
    "repository": {
        "type": "git",
        "url": "git:/github.com/sh4rkman/SquadCalc"
    },
    "homepage": "https://squadcalc.app/",
    "bugs": {
        "url": "https://github.com/sh4rkman/SquadCalc/issues"
    },
    "private": false,
    "jshintConfig": {
        "esversion": 7
    },
    "scripts": {
        "start": "webpack serve --open --config webpack.test.js",
        "build:prod": "webpack --config webpack.prod.js",
        "build:dev": "webpack --config webpack.dev.js",
        "test:all": "npm-run-all test:lint test:jshint test:htmlhint test:stylelint test:w3c",
        "test:lint": "eslint ./src/",
        "test:jshint": "jshint ./src/",
        "test:stylelint": "stylelint ./src/css/*.scss",
        "test:htmlhint": "htmlhint ./src/",
        "test:w3c": "html-validator ./src/"
    },
    "devDependencies": {
        "css-loader": "^6.7.3",
        "css-minimizer-webpack-plugin": "^3.4.1",
        "eslint": "^8.34.0",
        "glob-parent": ">=5.1.2",
        "html-loader": "^3.1.0",
        "html-webpack-plugin": "^5.5.0",
        "htmlhint": "^1.1.4",
        "jshint": "^2.13.6",
        "mini-css-extract-plugin": "^2.6.0",
        "node-sass": "^7.0.3",
        "npm-run-all": "^4.1.5",
        "postcss": ">=7.0.36",
        "purecss": "^3.0.0",
        "robotstxt-webpack-plugin": "^8.0.0",
        "sass-loader": "^12.6.0",
        "select2": "^4.1.0-rc.0",
        "style-loader": "^3.3.1",
        "stylelint": "^15.3.0",
        "stylelint-config-standard-scss": "^7.0.1",
        "tippy.js": "^6.3.7",
        "w3c-html-validator": "^1.3.2",
        "webpack": "^5.76.0",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.8.1",
        "webpack-jquery-ui": "^2.0.1",
        "webpack-merge": "^5.8.0"
    }
}